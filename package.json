{
  "name": "effect-jetstream",
  "version": "1.0.4",
  "description": "Effect-based client for the Bluesky Jetstream API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mepuka/effect-jetstream.git"
  },
  "keywords": [
    "bluesky",
    "jetstream",
    "effect",
    "atprotocol",
    "websocket",
    "streaming"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "effect-language-service patch",
    "typecheck": "tsc --noEmit",
    "harness": "bun run examples/harness.ts",
    "harness:live": "bun run examples/harness.ts --mode live --durationSec 60 --reportEverySec 5",
    "harness:replay": "bun run examples/harness.ts --mode replay --durationSec 30 --replayFile examples/fixtures/jetstream-sample.ndjson",
    "prepublishOnly": "bun run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "bun run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@effect/language-service": "^0.71.0",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@effect/platform": "^0.94.1",
    "effect": "^3.19.14"
  }
}
